{
  "name": "liminal",
  "version": "0.1.3",
  "description": "Privacy-native browser execution environment",
  "bin": {
    "liminal": "dist/cli/liminal.js"
  },
  "main": "dist/main/index.js",
  "scripts": {
    "build": "tsc && node scripts/copy-electron-assets.cjs && node scripts/generate-icon.cjs",
    "start": "npm run build && electron .",
    "dev": "tsc && electron .",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev:mvp": "tsc && node scripts/copy-electron-assets.cjs && electron dist/electron/main.js",
    "pack": "npm run build && electron-builder --config electron-builder.yml --publish never",
    "pack:release": "npm run build && electron-builder --config electron-builder.yml --publish always"
  },
  "keywords": [
    "privacy",
    "browser",
    "electron",
    "isolated-contexts"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}

